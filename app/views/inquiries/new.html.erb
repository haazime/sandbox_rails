<h1>
  New inquiry
</h1>
<section class="nav">
  <%= link_to 'List', inquiries_url %>
</section>
<hr>
<%= form_with model: @form, scope: :form, url: inquiries_url do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control', required: true %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: 'form-control' %>
  </div>
  <%= f.button class: 'btn btn-primary' %>
<% end %>
